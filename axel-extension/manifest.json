{
  "name": "Axel Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["https://*.linkedin.com/*"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
